{
  "name": "knockout.address",
  "version": "3.1.1",
  "description": "address bindings and extension for observable address objects",
  "main": "knockout.address.js",
  "scripts": {
    "test": "mocha",
    "lint": "eslint . --format=unix",
    "prepublish": "npm run lint && mocha",
    "devboiler": "grunt",
    "build": "webpack --progress && npm run devboiler",
    "watch": "webpack --progress --watch",
    "serve": "http-server demos -p 1341 -o -c-1",
    "start": "npm run build && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/civicsource/knockout.address.git"
  },
  "keywords": [
