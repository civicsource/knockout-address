    "knockout",
    "binding",
    "extension"
  ],
  "author": "CivicSource",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/civicsource/knockout.address/issues"
  },
  "homepage": "https://github.com/civicsource/knockout.address#readme",
  "devDependencies": {
    "babel-eslint": "6.x",
    "bootstrap": "3.x",
    "chai": "3.x",
    "eslint": "2.x",
    "eslint-plugin-filenames": "1.x",
    "grunt": "1.x",
    "grunt-cli": "1.x",
    "grunt-contrib-copy": "1.x",
    "http-server": "0.9.x",
    "jquery": "^3.0.0",
    "jquery-extend": "2.x",
    "mocha": "2.x",
    "webpack": "1.x"
  },
  "dependencies": {
    "html-loader": "^0.4.3",
    "knockout": "3.x",
    "knockout-template-loader": "1.x",
    "knockout.punches": "0.5.x",
    "knockout.validation": "2.x",
    "lodash.compact": "3.x",
